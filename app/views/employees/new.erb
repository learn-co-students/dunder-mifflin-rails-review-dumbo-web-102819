
<%= form_for @employee do |f| %>

  <%= f.label 'First_name' %>
  <%= f.text_field :first_name%>
  <br /> <br />

  <%= f.label 'Last_name' %>
  <%= f.text_field :last_name %>
  <br /> <br />

  <%= f.label 'Alias' %>
  <%= f.text_field :alias %>
  <br>
  <%= f.label 'Title' %>
  <%= f.text_field :title%>
  <%= f.label 'Img_Url' %>
  <%= f.text_field :img_url %>

  <%=f.collection_select :dog_id, Dog.all, :id, :name %>
  
  

  <%=f.submit%>

<% end %>